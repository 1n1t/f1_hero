{
	"presets": [
		"react",
		["env", {
			"targets": {
				"browsers": "last 2 versions"
			},
			"loose": true,
			"modules": false
		}],
		"stage-3"
	],
	"plugins": [
		"react-hot-loader/babel",
		"transform-async-to-generator",
		"babel-plugin-transform-class-properties",
		"transform-object-rest-spread"
	],
	"env": {
		"test": {
			"plugins": [
				"babel-plugin-transform-class-properties",
				"transform-es2015-modules-commonjs",
				"transform-async-to-generator"
			]
		},
		"production": {
			"plugins": [
				"transform-es2015-modules-commonjs"
			]
		}
	}
}
