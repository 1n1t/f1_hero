{
	"presets": [
		"react",
		["env", {
			"targets": {
				"browsers": "last 2 versions"
			},
			"loose": true,
			"modules": false
		}]
	],
	"plugins": [
		"react-hot-loader/babel",
		"transform-async-to-generator",
		"babel-plugin-transform-class-properties"
	],
	"env": {
		"test": {
			"plugins": [
				"transform-es2015-modules-commonjs",
				"transform-async-to-generator"
			]
		},
		"server": {
			"plugins": [
				"transform-es2015-modules-commonjs"
			]
		},
        "production": {
          "plugins": [
            "transform-es2015-modules-commonjs"
          ]
        }
	}
}
