{
	"extends": ["airbnb", "prettier", "prettier/react"],
	"plugins": ["prettier"],
	"rules": {
		"prettier/prettier": ["error"],
		"function-paren-newline": "off",
		"no-underscore-dangle": "off",
		"import/no-unresolved": [2, { "ignore": ["App", "Root"] }],
		"import/extensions": [2, { "js": "never" }]
	},
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaVersion": 2016,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"env": {
		"es6": true,
		"browser": true,
		"node": true,
		"jest": true
	}
}
